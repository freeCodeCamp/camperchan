    "ffmpeg-static": "^4.2.6",
    "git-repo-info": "^2.1.1",
    "highlight.js": "^10.0.3",
    "luxon": "^1.24.1",
    "mongoose": "^5.9.20",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/common-tags": "^1.8.0",
    "@types/highlightjs": "^9.12.0",
    "@types/jest": "^26.0.0",
    "@types/luxon": "^1.24.1",
    "@types/mongoose": "^5.7.29",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.0.1",
    "@types/strip-ansi": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
